{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "eslint",
    "generate-types": "node scripts/generate-api-types.mjs",
    "generate-types:file": "node scripts/generate-api-types.mjs openapi.json",
    "audit:validate": "ts-node scripts/validate-audit-integration.ts",
    "audit:test": "ts-node scripts/test-lens-audit.ts",
    "audit:seed": "ts-node scripts/seed-audit-demo.ts",
    "audit:setup": "prisma migrate dev && npm run audit:validate && npm run audit:seed",
    "audit:reset": "prisma migrate reset --force && npm run audit:seed",
    "compliance:test": "ts-node scripts/test-compliance-integration.ts",
    "compliance:snapshot": "curl -X POST http://localhost:3000/api/v1/compliance -H 'Content-Type: application/json' -d '{\"action\":\"storeSnapshot\"}'",
    "compliance:recalculate": "curl -X POST http://localhost:3000/api/v1/compliance -H 'Content-Type: application/json' -d '{\"action\":\"recalculate\"}'",
    "test:dashboards": "ts-node scripts/test_dashboard_flow.ts"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tanstack/react-query": "^5.0.0",
    "@types/pg": "^8.16.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "d3": "^7.8.5",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.468.0",
    "next": "^16.0.7",
    "pg": "^8.16.3",
    "react": "19.2.0",
    "react-day-picker": "^8.10.0",
    "react-dom": "19.2.0",
    "react-simple-maps": "^3.0.0",
    "recharts": "^2.10.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.3.0",
    "uuid": "^9.0.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/d3": "^7.4.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.6",
    "openapi-typescript": "^7.13.0",
    "prisma": "^7.4.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
